<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:primaryType="cq:Panel"
    title="SEO">
    <items jcr:primaryType="cq:WidgetCollection">
        <title
            jcr:primaryType="cq:Widget"
            fieldLabel="HTML Title"
            name="./title"
            xtype="textfield"/>
        <description
            jcr:primaryType="cq:Widget"
            fieldLabel="Description"
            name="./description"
            xtype="textfield"/>
        <keywords
            jcr:primaryType="cq:Widget"
            fieldLabel="Keywords"
            name="./keywords"
            xtype="textfield"/>
        <noFollow
            jcr:primaryType="cq:Widget"
            fieldLabel="No Follow"
            inputValue="true"
            name="./noFollow"
            type="checkbox"
            xtype="selection"/>
        <noIndex
            jcr:primaryType="cq:Widget"
            fieldLabel="No Index"
            inputValue="true"
            name="./noIndex"
            type="checkbox"
            xtype="selection"/>
    </items>
</jcr:root>
