<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0"
    jcr:description="Newsletter Detail Template -T.5.6  "
    jcr:primaryType="cq:Template"
    jcr:title="Newsletter Detail Template"
    allowedPaths="/content(/.*)?"
    ranking="{Long}150">
    <jcr:content
    	contentType="newsletter"
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dupont/phoenix/components/pages/newsletterdetail">
      	
        <linklistdynamic-newsletter
        	contentType="newsletter"
        	jcr:primaryType="nt:unstructured"
			sling:resourceType="dupont/phoenix/components/linklistdynamic"/>
        <linklistdynamic-products
        	contentType="product"
        	jcr:primaryType="nt:unstructured"
			sling:resourceType="dupont/phoenix/components/linklistdynamic"/>			        
        <bodycopy
        	jcr:primaryType="nt:unstructured"
			sling:resourceType="dupont/phoenix/components/bodycopy"/>
	</jcr:content>
</jcr:root>
