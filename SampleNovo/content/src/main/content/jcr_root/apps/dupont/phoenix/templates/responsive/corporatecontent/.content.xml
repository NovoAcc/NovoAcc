<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Responsive Corporate Content Landing Template - T.7.1"
    jcr:primaryType="cq:Template"
    jcr:title="Responsive Corporate Content Landing Template"
    allowedPaths="/content(/.*)?"
    ranking="{Long}380">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dupont/phoenix/components/responsive/pages/corporatecontent"
        contentType="corporatecontent"
        isQuickLink="true">
        <featuredb
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/featuredb">
            <selectedtool
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/linklistcurated"
                isQuickLink="true"/>
        </featuredb>
        <hlm-articles
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/hlm"
            contentType="article"/>
        <featuredc
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/featuredb">
            <selectedtool
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/linklistcurated"
                isQuickLink="true"/>
        </featuredc>
        <contextualfooter
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/footers/contextualfooter"
            moreLinksEnabled="false"/>
    </jcr:content>
</jcr:root>
