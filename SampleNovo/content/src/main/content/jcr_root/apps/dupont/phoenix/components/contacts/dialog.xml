                                fieldLabel="List of Contacts"
                                fieldDescription= "When entering phone numbers, please include the correspondent country code"
                                name="./contactColumnItems"
                                xtype="custommultifield">
                                <fieldConfig
                                    jcr:primaryType="cq:Widget"
                                    limit="3"
                                    xtype="customcontactlist"/>
                                <listeners jcr:primaryType="nt:unstructured"/>
                            </contactsListItem>
                        </items>
                    </availablecontacts>
                </items>
            </tab1>
        </items>
    </items>
