<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Media Center Landing Page - T.10.0"
    jcr:primaryType="cq:Template"
    jcr:title="Media Center Landing Page"
    allowedPaths="[/content(/.*)?]"
    ranking="{Long}440">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dupont/phoenix/components/responsive/pages/mcLandingPage">
        <mcpressrelease-1
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mcpressreleasehlm"
            contentType="pressrelease"/>
        <mcpressrelease-2
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mcpressreleasehlm"
            contentType="pressrelease"/>
        <mcnewshlm-1
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mchlm"
            contentType="mcdailynews"/>
        <mcpresskit-1
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mchlm"
            contentType="presskit"/>
        <multimedia
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/multimedia"
            contentType="multimedia"/>
        <mcpressrelease-3
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mcpressreleasehlm"
            contentType="pressrelease"/>
        <mcpressrelease-4
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mcpressreleasehlm"
            contentType="pressrelease"/>
        <mcnewshlm-2
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/mchlm"
            contentType="mcdailynews"/>
        <featuredb
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/responsive/featuredb">
            <selectedtool
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/linklistcurated"
                isCurated="true"/>
        </featuredb>
        <viewallcontent jcr:primaryType="nt:unstructured">
            <querybuilder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/viewall/querybuilder">
                <left jcr:primaryType="nt:unstructured">
                    <options
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dupont/phoenix/components/viewall/predicates/options"/>
                </left>
            </querybuilder>
        </viewallcontent>
        <introbodytext
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/introbodytext"
            hideInlineCallout="{Boolean}true"/>
    </jcr:content>
</jcr:root>
