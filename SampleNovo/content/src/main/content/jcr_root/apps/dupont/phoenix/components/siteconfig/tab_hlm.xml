        <maxdisplayitemswithcallout
            jcr:primaryType="cq:Widget"
            allowDecimals="false"
            allowNegative="false"
            defaultValue="6"
            fieldLabel="Max Display Items with Row Callout Visible"
            name="./../../siteconfig/hlm/maxDisplayItemsWithCallout"
            value="6"
            xtype="numberfield"/>
        <maxdisplayitemswithoutcallout
            jcr:primaryType="cq:Widget"
            allowDecimals="false"
            allowNegative="false"
            defaultValue="12"
            fieldLabel="Max Display Items with Row Callout Hidden"
            name="./../../siteconfig/hlm/maxDisplayItemsWithoutCallout"
            value="12"
            xtype="numberfield"/>
        <maxitemsforviewallwithcallout
            jcr:primaryType="cq:Widget"
            allowDecimals="false"
            allowNegative="false"
            defaultValue="2"
            fieldDescription="Show view all and carousel if number of items are more than entered within this box. This is used when row callout is activated."
            fieldLabel="Max Items for View All Activation with Row Callout Visible"
            name="./../../siteconfig/hlm/maxItemsViewAllWithCallout"
            value="2"
            xtype="numberfield"/>
        <maxitemsforviewallwithoutcallout
            jcr:primaryType="cq:Widget"
            allowDecimals="false"
            allowNegative="false"
            defaultValue="4"
            fieldDescription="Show view all if number of items are more than entered within this box. This is used when row callout is not activated."
            fieldLabel="Max Items for View All Activation with Row Callout Hidden"
            name="./../../siteconfig/hlm/maxItemsViewAllWithoutCallout"
            value="4"
            xtype="numberfield"/>
