<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:description="Uses And Applications Template - T.4.2"
    jcr:primaryType="cq:Template"
    jcr:title="Uses And Applications Template"
    allowedPaths="[/content(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        jcr:primaryType="cq:PageContent"
        sling:resourceType="dupont/phoenix/components/pages/usesandapplications"
        contentType="usesapplication"
		videoMediaGalleryUpgraded="{Boolean}true"
		imageMediaGalleryUpgraded="{Boolean}true"
        isCurated="true">
        <featuredb
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/featuredb">
            <selectedtool
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/linklistcurated"
                isCurated="true"/>
        </featuredb>
        <hlm-products
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/hlm"
            contentType="product"
            recommendedContentTypes="product,subproduct"/>
        <hlm-brands
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/hlm"
            contentType="brand"
            recommendedContentTypes="brand,subbrand"/>
        <featuredc
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/featuredb">
            <selectedtool
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/linklistcurated"
                isQuickLink="true"/>
        </featuredc>
        <hlm-articles
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/hlm"
            contentType="article"/>
        <hlm-casestudies
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/hlm"
            contentType="casestudy"/>
        <hlm-industries
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/hlm"
            contentType="industry"
            recommendedContentTypes="industry,subindustry"/>
        <hlm-subindustries
            jcr:primaryType="nt:unstructured"
            sling:resourceType="dupont/phoenix/components/hlm"
            contentType="subindustry"/>
        <viewallcontent jcr:primaryType="nt:unstructured">
            <querybuilder
                jcr:primaryType="nt:unstructured"
                sling:resourceType="dupont/phoenix/components/viewall/querybuilder">
                <left jcr:primaryType="nt:unstructured">
                    <options
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="dupont/phoenix/components/viewall/predicates/options"/>
                </left>
            </querybuilder>
        </viewallcontent>
    </jcr:content>
</jcr:root>
