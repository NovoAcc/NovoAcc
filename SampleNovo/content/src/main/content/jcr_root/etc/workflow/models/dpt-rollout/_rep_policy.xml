<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow
        jcr:primaryType="rep:GrantACE"
        rep:principalName="dpt-contributor"
        rep:privileges="{Name}[crx:replicate,jcr:versionManagement,jcr:nodeTypeManagement,jcr:modifyProperties,jcr:addChildNodes,jcr:lockManagement]"/>
    <allow0
        jcr:primaryType="rep:GrantACE"
        rep:glob="*/jcr:content*"
        rep:principalName="dpt-contributor"
        rep:privileges="{Name}[jcr:removeNode,jcr:removeChildNodes,jcr:nodeTypeManagement,jcr:addChildNodes]"/>
</jcr:root>
